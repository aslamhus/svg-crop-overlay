  },
  "homepage": "https://github.com/aslamhus/svg-crop-overlay#readme",
  "dependencies": {
    "html-webpack-plugin": "^5.5.0",
    "terser-webpack-plugin": "^5.3.6",
    "webpack": "^5.75.0",
    "webpack-dev-server": "^4.11.1",
    "webpack-merge": "^5.8.0"
  },
